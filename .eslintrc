{
 "extends": ["eslint:recommended", "airbnb-base"],
 "parser": "babel-eslint",
 "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
 },
 "globals": {
     "window": true,
     "document": true,
     "console": true,
     "Symbol": true,
     "$": true,
     "Proxy": true,
     "Snap": true    
 },
 "rules": {
     "no-console":"off",
     "linebreak-style":["error", "windows"],
     "func-names": 0,
     "no-plusplus":"off",
     "no-restricted-syntax":"off"
 }
}